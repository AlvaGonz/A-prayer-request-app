{
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/og",
            "destination": "/api/og.jsx"
        },
        {
            "source": "/api/:path*",
            "destination": "https://prayer-board-api.onrender.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}